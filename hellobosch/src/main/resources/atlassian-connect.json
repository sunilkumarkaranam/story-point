{
  "key": "com.thanh.connect.springboot.hellobosch",
  "baseUrl": "http://localhost:8080",
  "name": "Hellobosch",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": ["READ","ACT_AS_USER"],
  "apiVersion": 1,
  "modules": {
    "generalPages": [

      {
        "url": "/callapi",
        "key": "callapi",
        "location": "system.top.navigation.bar",
        "name": {
          "value": "List issue - Spring Boot"
        }
      }
    ]
  }
}
